	public Cone(int width, int height){
		
	}
	
	@Override
	public void draw(Graphics g) {
		// cone consists of a rounded rectange and two circles
		int center_x;
		int center_y;
		
		// rounded rectangle
		
		// first circle
		g.setColor(Color.white);
		g.fillOval(x, y, width, height);
		
		
	}
}
