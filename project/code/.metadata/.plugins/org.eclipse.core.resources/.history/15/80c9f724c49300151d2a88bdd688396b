package scheduler;

import java.util.ArrayList;
import java.util.List;

/**
 * runs tasks one at a time
 * 
 * @author element
 *
 */
public class Processor {
	public Task task;
	public List<Task> task_queue;

	public Processor() {
		this.task = new Task();
		this.task_queue = new ArrayList<Task>();
	}
}
