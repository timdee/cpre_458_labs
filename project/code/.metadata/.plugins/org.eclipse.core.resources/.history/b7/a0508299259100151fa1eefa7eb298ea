package characters;

import java.awt.Color;

public class Car extends Character {
	public int speed;

	public Car() {
		super();
		reset();
	}

	/**
	 * copy constructor
	 */
	public Car(Car car) {
		super(car);
	}

	public void reset() {
		this.color = Color.BLUE;
		this.width = 100;
		this.height = 200;
		this.x_pos = 0;
		this.y_pos = 0;
		this.speed = 0;
	}

	/**
	 * ask the car to draw itself
	 */
	public void draw() {

	}
}
