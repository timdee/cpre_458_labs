package characters;

import java.awt.Color;

public class Car extends Character {
	public Color color;
	public int width;
	public int height;
	public int x_pos;
	public int y_pos;

	public Car() {
		this.color = Color.BLUE;
		this.width = 100;
		this.height = 200;
		this.x_pos = 0;
		this.y_pos = 0;
	}
}
